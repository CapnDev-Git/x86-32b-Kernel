# Bochs configuration file
romimage: file=/usr/share/bochs/BIOS-bochs-latest
vgaromimage: file=/usr/share/bochs/VGABIOS-lgpl-latest
boot: cdrom
memory: 32MB
ata0-master: type=cdrom, path=my_os.iso, status=inserted
log: bochsout.txt
panic: action=ask
error: action=report
info: action=report
debug: action=ignore
debugger_log: bochsdebug.txt
debugger: enabled
port_e9_hack: enabled=1
